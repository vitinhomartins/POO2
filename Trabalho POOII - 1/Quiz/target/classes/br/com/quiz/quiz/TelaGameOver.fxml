<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.quiz.quiz.Controller.GameOverController"> <children>
    <VBox alignment="CENTER" layoutX="50.0" layoutY="50.0" prefHeight="200.0" prefWidth="400.0" spacing="20.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">

        <Label text="GAME OVER" textFill="RED">
            <font>
                <Font name="System Bold" size="48.0" />
            </font>
        </Label>

        <Label fx:id="lblPontuacaoFinal" text="Alternativa errada :(">
            <font>
                <Font size="18.0" />
            </font>
        </Label>

        <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#voltarParaInicio" text="VOLTAR PARA O MENU PRINCIPAL">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </VBox>
</children>
</AnchorPane>
